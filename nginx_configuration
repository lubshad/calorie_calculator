# Nginx configuration for zayanfitness.in (serve only index.html)
server {
    listen 80;
    listen [::]:80;
    server_name zayanfitness.in;

    root /var/www/calorie_calculator;
    index index.html;

    location / {
        default_type text/html;
        try_files /index.html =404;
    }

    access_log /var/log/nginx/calorie_calculator.access.log;
    error_log /var/log/nginx/calorie_calculator.error.log;
}